.data-list {
    overflow: hidden;
}
.data-list .items {
    width: calc(25% - 60rpx);
    float: left;
    padding: 30rpx;
}
.items-content {
    border-radius: 50%;
    padding: 20rpx;
}
.data-list .items image {
    width: 80rpx !important;
    height: 80rpx !important;
}
.data-list .items .title {
    margin-top: 10rpx;
    font-size: 32rpx;
}